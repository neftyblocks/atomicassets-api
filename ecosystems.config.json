{
  "apps": [
    {
      "name": "eosio-contract-api-filler",
      "script": "build/bin/filler.js",

      "instances": 1,

      "autorestart": true,
      "kill_timeout": 60000,

      "watch": false,

      "env": {
        "NODE_ENV": "production"
      }
    },
    {
      "name": "eosio-contract-api-server",
      "script": "build/bin/server.js",

      "instances": 1,

      "autorestart": true,
      "exp_backoff_restart_delay": 100,

      "watch": false,

      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}
